{
  "extends": ["google", "plugin:react/recommended"],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "linebreak-style": 0,
    "quotes": [
      1,
      "double",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "comma-dangle": 0,
    "no-unused-vars": 1,
    "prefer-const": 1,
    "spaced-comment": 1,
    "arrow-parens": 0,
    "require-jsdoc": 0,
    "object-curly-spacing": 0,
    "max-len": ["error", {"code": 120}],
    "prefer-rest-params": 0,
    "space-before-function-paren": 0,
    "new-cap": 0,
    "no-invalid-this": 0,
    "quote-props": 0
  }
}
